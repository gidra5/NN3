{
  "name": "mini-nn",
  "version": "1.0.0",
  "description": "A neural network library with basic functionality for creating a working one and adjusting it dynamicly.",
  "main": "./build/src.js",
  "dependencies": {
    "@types/node": "^14.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "test": "echo 'Testing...' && npx eslint ./ && node ./tests/test-learning.js && node ./tests/test-computation.js && node ./tests/test-save-load.js && node ./tests/test-remove-add.js",
    "clear": "rm -r ./build || true",
    "build": "echo 'Building...' && npx tsc -t 'ESNEXT' --strictFunctionTypes --noImplicitReturns --outDir ./build ./src/src.ts || true",
    "install": "node in.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gidra5/NN3.git"
  },
  "keywords": [
    "neural",
    "network",
    "nn",
    "lightweight"
  ],
  "author": "gidra5",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gidra5/NN3/issues"
  },
  "homepage": "https://github.com/gidra5/NN3#readme"
}